{
	"name": "元気が出るスイッチ",
	"manifest_version": 3,
	"version": "0.1",
	"permissions": ["activeTab","scripting"],
	"background": {
		"service_worker": "alert.js"
	},
	"action": {
		"default_icon": {
			"16": "images/16.png",
			"24": "images/16.png",
			"32": "images/16.png",
			"48": "images/16.png"
		},
		"default_popup": "popup.html"
	}
}